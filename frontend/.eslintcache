ate(0);\r\n  const [comment, setComment] = useState('');\r\n\r\n  const dispatch = useDispatch();\r\n  const alert = useAlert();\r\n\r\n  const {\r\n    loading,\r\n    error,\r\n    product,\r\n    similarProducts = [],\r\n  } = useSelector((state) => state.productDetails);\r\n  const { user } = useSelector((state) => state.auth);\r\n  const { error: reviewError, success } = useSelector(\r\n    (state) => state.newReview\r\n  );\r\n\r\n  const [loadedImg, setLoadedImg] = useState(false);\r\n\r\n  useEffect(() => {\r\n    if (loading) {\r\n      setTimeout(() => {\r\n        setLoadedImg(true);\r\n      }, 1000);\r\n    }\r\n  }, [loading]);\r\n\r\n  useEffect(() => {\r\n    dispatch(getProductDetails(match.params.id));\r\n\r\n    if (error) {\r\n      alert.error(error);\r\n      dispatch(clearErrors());\r\n    }\r\n\r\n    if (reviewError) {\r\n      alert.error(reviewError);\r\n      dispatch(clearErrors());\r\n    }\r\n\r\n    if (success) {\r\n      alert.success('Reivew posted successfully');\r\n      dispatch({ type: NEW_REVIEW_RESET });\r\n    }\r\n  }, [dispatch, alert, error, reviewError, match.params.id, success]);\r\n\r\n  const addToCart = () => {\r\n    dispatch(addItemToCart(match.params.id, quantity));\r\n    alert.success('Item Added to Cart');\r\n  };\r\n\r\n  const increaseQty = () => {\r\n    const count = document.querySelector('.count');\r\n\r\n    if (count.valueAsNumber >= product.stock) return;\r\n\r\n    const qty = count.valueAsNumber + 1;\r\n    setQuantity(qty);\r\n  };\r\n\r\n  const decreaseQty = () => {\r\n    const count = document.querySelector('.count');\r\n\r\n    if (count.valueAsNumber <= 1) return;\r\n\r\n    const qty = count.valueAsNumber - 1;\r\n    setQuantity(qty);\r\n  };\r\n\r\n  function setUserRatings() {\r\n    const stars = document.querySelectorAll('.star');\r\n\r\n    stars.forEach((star, index) => {\r\n      star.starValue = index + 1;\r\n\r\n      ['click', 'mouseover', 'mouseout'].forEach(function (e) {\r\n        star.addEventListener(e, showRatings);\r\n      });\r\n    });\r\n\r\n    function showRatings(e) {\r\n      stars.forEach((star, index) => {\r\n        if (e.type === 'click') {\r\n          if (index < this.starValue) {\r\n            star.classList.add('orange');\r\n\r\n            setRating(this.starValue);\r\n          } else {\r\n            star.classList.remove('orange');\r\n          }\r\n        }\r\n\r\n        if (e.type === 'mouseover') {\r\n          if (index < this.starValue) {\r\n            star.classList.add('yellow');\r\n          } else {\r\n            star.classList.remove('yellow');\r\n          }\r\n        }\r\n\r\n        if (e.type === 'mouseout') {\r\n          star.classList.remove('yellow');\r\n        }\r\n      });\r\n    }\r\n  }\r\n\r\n  const reviewHandler = () => {\r\n    const formData = new FormData();\r\n\r\n    formData.set('rating', rating);\r\n    formData.set('comment', comment);\r\n    formData.set('productId', match.params.id);\r\n\r\n    dispatch(newReview(formData));\r\n  };\r\n\r\n  return (\r\n    <Fragment>\r\n      {/* {loading ? (\r\n        <Loader />\r\n      ) : ( */}\r\n      <Fragment>\r\n        <MetaData title={product.name} />\r\n        <div className=\"row d-flex justify-content-around\">\r\n          <div className=\"col-12 col-lg-5 img-fluid\" id=\"product_image\">\r\n            {loadedImg ? (\r\n              <Carousel pause=\"hover\">\r\n                {product.images &&\r\n                  product.images.map((image) => (\r\n                    <Carousel.Item key={image.public_id}>\r\n                      <Image\r\n                        fluid\r\n                        style={{ maxHeight: '400px' }}\r\n                        className=\"d-block w-100 rounded border\"\r\n                        src={image.url}\r\n                        alt={product.title}\r\n                      />\r\n                    </Carousel.Item>\r\n                  ))}\r\n              </Carousel>\r\n            ) : (\r\n              <Skeleton height={400} />\r\n            )}\r\n          </div>\r\n\r\n          <div className=\"col-12 col-lg-5 mt-5\">\r\n            {loadedImg ? (\r\n              <h3>{product.name}</h3>\r\n            ) : (\r\n              <Skeleton count={2} height={30} />\r\n            )}\r\n\r\n            {loadedImg ? (\r\n              <p id=\"product_id\">Product # {product._id}</p>\r\n            ) : (\r\n              <Skeleton count={1} height={10} width={250} />\r\n            )}\r\n\r\n            <hr />\r\n\r\n            {loadedImg ? (\r\n              <>\r\n                <div className=\"rating-outer\">\r\n                  <div\r\n                    className=\"rating-inner\"\r\n                    style={{ width: `${(product.ratings / 5) * 100}%` }}\r\n                  ></div>\r\n                </div>\r\n                <span id=\"no_of_reviews\">({product.numOfReviews} Reviews)</span>\r\n              </>\r\n            ) : (\r\n              <Skeleton count={1} height={10} width={250} />\r\n            )}\r\n            <hr />\r\n\r\n            {loadedImg ? (\r\n              <p id=\"product_price\">${product.price}</p>\r\n            ) : (\r\n              <Skeleton count={1} height={30} width={150} />\r\n            )}\r\n            <div className=\"d-flex justify-content-between\">\r\n              {loadedImg ? (\r\n                <div className=\"d-flex w-50\">\r\n                  <span className=\"btn btn-danger minus\" onClick={decreaseQty}>\r\n                    -\r\n                  </span>\r\n\r\n                  <input\r\n                    type=\"number\"\r\n                    className=\"form-control count d-inline text-center\"\r\n                    value={quantity}\r\n                    readOnly\r\n                  />\r\n\r\n                  <span className=\"btn btn-primary plus\" onClick={increaseQty}>\r\n                    +\r\n                  </span>\r\n                </div>\r\n              ) : (\r\n                <Skeleton count={1} height={40} width={250} />\r\n              )}\r\n              {loadedImg ? (\r\n                <Button\r\n                  className=\"btn btn-success d-inline ml-4\"\r\n                  disabled={product.stock === 0}\r\n                  onClick={addToCart}\r\n                >\r\n                  Add to Cart\r\n                </Button>\r\n              ) : (\r\n                <Skeleton count={1} height={40} width={100} />\r\n              )}\r\n            </div>\r\n\r\n            <hr />\r\n\r\n            {loadedImg ? (\r\n              <p>\r\n                Status:{' '}\r\n                <span\r\n                  id=\"stock_status\"\r\n                  className={product.stock > 0 ? 'text-success' : 'text-danger'}\r\n                >\r\n                  {product.stock > 0 ? 'In Stock' : 'Out of Stock'}\r\n                </span>\r\n              </p>\r\n            ) : (\r\n              <Skeleton count={1} height={20} width={150} />\r\n            )}\r\n\r\n            <hr />\r\n\r\n            {loadedImg ? (\r\n              <h4 className=\"mt-2\">Description:</h4>\r\n            ) : (\r\n              <Skeleton count={1} height={40} width={200} />\r\n            )}\r\n            {loadedImg ? (\r\n              <p>{product.description}</p>\r\n            ) : (\r\n              <Skeleton count={3} height={30} />\r\n            )}\r\n            <hr />\r\n            {loadedImg ? (\r\n              <p id=\"product_seller mb-3\">\r\n                Sold by: <strong>{product.seller}</strong>\r\n              </p>\r\n            ) : (\r\n              <Skeleton count={1} height={20} width={150} />\r\n            )}\r\n\r\n            {user ? (\r\n              <>\r\n               {loadedImg? <Button\r\n                  className=\"btn btn-success mt-4\"\r\n                  data-toggle=\"modal\"\r\n                  data-target=\"#ratingModal\"\r\n                  onClick={setUserRatings}\r\n                >\r\n                  Submit Your Review\r\n                </Button>:(\r\n                  <Skeleton count={1} height={50} width={200} />  \r\n                )}\r\n              </>\r\n            ) : (\r\n              <div className=\"alert alert-danger mt-5\" type=\"alert\">\r\n                Login to post your review.\r\n              </div>\r\n            )}\r\n\r\n            <div className=\"row mt-2 mb-5\">\r\n              <div className=\"rating w-50\">\r\n                <div\r\n                  className=\"modal fade\"\r\n                  id=\"ratingModal\"\r\n                  tabIndex=\"-1\"\r\n                  role=\"dialog\"\r\n                  aria-labelledby=\"ratingModalLabel\"\r\n                  aria-hidden=\"true\"\r\n                >\r\n                  <div className=\"modal-dialog\" role=\"document\">\r\n                    <div className=\"modal-content\">\r\n                      <div className=\"modal-header\">\r\n                        <h5 className=\"modal-title\" id=\"ratingModalLabel\">\r\n                          Submit Review\r\n                        </h5>\r\n                        <Button\r\n                          className=\"btn-danger\"\r\n                          data-dismiss=\"modal\"\r\n                          aria-label=\"Close\"\r\n                        >\r\n                          <span aria-hidden=\"true\">&times;</span>\r\n                        </Button>\r\n                      </div>\r\n                      <div className=\"modal-body\">\r\n                        <ul className=\"stars\">\r\n                          <li className=\"star\">\r\n                            <i className=\"fa fa-star\"></i>\r\n                          </li>\r\n                          <li className=\"star\">\r\n                            <i className=\"fa fa-star\"></i>\r\n                          </li>\r\n                          <li className=\"star\">\r\n                            <i className=\"fa fa-star\"></i>\r\n                          </li>\r\n                          <li className=\"star\">\r\n                            <i className=\"fa fa-star\"></i>\r\n                          </li>\r\n                          <li className=\"star\">\r\n                            <i className=\"fa fa-star\"></i>\r\n                          </li>\r\n                        </ul>\r\n\r\n                        <textarea\r\n                          name=\"review\"\r\n                          id=\"review\"\r\n                          className=\"form-control mt-3\"\r\n                          value={comment}\r\n                          onChange={(e) => setComment(e.target.value)}\r\n                        ></textarea>\r\n\r\n                        <Button\r\n                          className=\"btn btn-success my-3 float-right  px-4 text-white\"\r\n                          onClick={reviewHandler}\r\n                          data-dismiss=\"modal\"\r\n                          aria-label=\"Close\"\r\n                        >\r\n                          Submit\r\n                        </Button>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <Container>\r\n          <h3 className=\"text-center mt-4\">Similar Products</h3>\r\n\r\n          <Row>\r\n            {similarProducts.map((product) => (\r\n              <Product\r\n                key={product._id}\r\n                product={product}\r\n                col={4}\r\n                small={true}\r\n              />\r\n            ))}\r\n          </Row>\r\n        </Container>\r\n        <Container>\r\n          {product.reviews && product.reviews.length > 0 && (\r\n            <ListReviews reviews={product.reviews} />\r\n          )}\r\n        </Container>\r\n      </Fragment>\r\n      {/* )} */}\r\n    </Fragment>\r\n  );\r\n};\r\